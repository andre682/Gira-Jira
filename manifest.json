{
    "manifest_version": 3,
    "name": "Gira Jira",
    "version": "1.0",
    "description": "Plugin que gira o jira",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
        "default_title": "Gira Jira"
    },
  "background": {
    "service_worker": "background.js"
  },
    "permissions": [
        "activeTab", "scripting"
    ]
}
